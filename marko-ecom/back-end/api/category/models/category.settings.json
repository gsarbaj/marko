{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "name": "category"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "url": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "icon": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "qty": {
      "type": "string",
      "required": true,
      "default": "1359"
    }
  }
}
